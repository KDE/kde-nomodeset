#: main.cpp:68
#, kde-format
msgctxt "@info:status"
msgid "Unknown error code: %1"
msgstr "Άγνωστος κωδικός σφάλματος: %1"

#: main.cpp:182
#, kde-format
msgctxt "@title"
msgid "Safe Graphics Mode Warning"
msgstr "Προειδοποίηση ασφαλούς λειτουργίας γραφικών"

#: main.cpp:183
#, kde-format
msgctxt "@title systray tooltip title"
msgid "Safe Graphics Mode"
msgstr "Ασφαλής λειτουργία γραφικών"

#: main.cpp:185
#, kde-format
msgctxt "@info:tooltip"
msgid ""
"The system currently runs in Safe Graphics Mode - graphics card and display "
"performance may be impaired"
msgstr ""
"Το σύστημα τώρα εκτελείται σε ασφαλή λειτουργία γραφικών - η κάρτα γραφικών "
"και η απόδοση της οθόνης ίσως επηρςαστούν"

#: polkit/helper.cpp:20
#, kde-kuit-format
msgctxt "@info:status"
msgid "Configuration file <filename>%1</filename> not found."
msgstr "Αδυναμία εύρεσης του αρχείου διαμόρφωσης <filename>%1</filename>."

#: polkit/helper.cpp:25
#, kde-kuit-format
msgctxt "@info:status"
